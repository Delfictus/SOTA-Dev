show surface, pocket_13_surface
set surface_color, slate, pocket_13_surface
set transparency, 0.7, pocket_13_surface
pseudoatom pocket_13_center, pos=[22.338, -4.537, -12.568]
show spheres, pocket_13_center
set sphere_scale, 0.5, pocket_13_center
color red, pocket_13_center
group pocket_13, pocket_13_lining pocket_13_surface pocket_13_center pocket_13_catalytic pocket_13_hydrophobic

group all_pockets, pocket_1 pocket_2 pocket_3 pocket_4 pocket_5 pocket_6 pocket_7 pocket_8 pocket_9 pocket_10 pocket_11 pocket_12 pocket_13

# ========== Color Legend ==========
# Magenta = Catalytic residues (GLU, ASP, HIS, SER, CYS, LYS)
# Green = Aromatic residues (PHE, TYR, TRP) - pi-stacking
# Yellow = Hydrophobic residues (ALA, VAL, LEU, ILE, MET, PRO)
# Slate surface = Pocket cavity
# Red sphere = Druggable site centroid
# Gray sphere = Non-druggable site centroid

# ========== Usage Tips ==========
# To view specific pocket: disable all_pockets, enable pocket_N
# To zoom on pocket: zoom pocket_N_lining
# To show H-bonds: select donors, pocket_N_lining; distance hbonds, donors, acceptors, 3.5
# To label residues: label pocket_N_lining and name CA, "%s%s" % (resn, resi)
